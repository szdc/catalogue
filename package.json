{
  "name": "catalogue",
  "description": "Parse supermarket catalogues from SaleFinder",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/szdc/catalogue#readme",
  "author": "Jack Willis-Craig",
  "bugs": {
    "url": "https://github.com/szdc/catalogue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szdc/catalogue.git"
  },
  "keywords": [
    "salefinder",
    "supermarket",
    "catalogue"
  ],
  "scripts": {
    "build": "tsc",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "tslint --project tsconfig.json --config tslint.json",
    "test": "nyc --reporter=text mocha",
    "type-check": "tsc --project tsconfig.json --noEmit",
    "watch": "tsc -w"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/cheerio": "^0.22.7",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-airbnb": "^5.9.2",
    "typescript": "^2.9.1"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "coverage/**",
      "lib/**",
      "test/**",
      "**/*.d.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
